<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".lunch">

    <Button
        android:id="@+id/lunchMenu1"
        android:layout_width="238dp"
        android:layout_height="41dp"
        android:layout_marginTop="76dp"
        android:text="@string/lunchMenu1"
        app:layout_constraintEnd_toEndOf="@+id/lunchMenu2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/lunchMenu2"
        app:layout_constraintTop_toBottomOf="@+id/lunchMenu2" />

    <Button
        android:id="@+id/lunchMenu2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="176dp"
        android:text="@string/lunchMenu2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/lunchMenu3"
        android:layout_width="240dp"
        android:layout_height="51dp"
        android:layout_marginTop="88dp"
        android:text="@string/lunchMenu3"
        app:layout_constraintEnd_toEndOf="@+id/lunchMenu1"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/lunchMenu1"
        app:layout_constraintTop_toBottomOf="@+id/lunchMenu1" />
</androidx.constraintlayout.widget.ConstraintLayout>